@{
    ViewBag.Title = Maps.PryHouse;

    var map = Maps.PryHouse;
    var dateTime = Maps.DateTimes[map];
    var image = Url.Content($"~/img/maps/{map}_spawn.png");
    var imageLoading = Url.Content($"~/img/maps/{map}_loading.png");

    var mapInfo = MapsInfo.Parameters[map];
}

@section navigation {
    @await Html.PartialAsync("_MapsNav")
}

<h2 class="contentTitle" id="tabTitle">Pry House</h2>

<div id="mapHistory" class="fancyClearBoth">

    <h2 class="text-right">History</h2>

</div>

<h2 class="text-right">@dateTime</h2>
<a href="@image" target="_blank">
    <img src="@image" class="imageMaxWidth">
</a>

<p></p>

<a href="@imageLoading" target="_blank">
    <img src="@imageLoading" class="imageMaxWidth">
</a>

@await Html.PartialAsync("_MapTeams", mapInfo)

<div id="galleryImagesContainer">
    <h1 class="text-center">
        Ingame images of the map
    </h1>

    <div id="galleryImages">
        <a href="#" class="active" data-src="https://warofrights.com/Pages_FieldReports/Update166/Images/Carlton_battery_1-min.jpg">
            <img class="galleryImageContainer worTightBorder" src="https://warofrights.com/Pages_FieldReports/Update166/Images/Carlton_battery_1-min.jpg" alt="Campbell’s Battery (USA), Hagerstown Turnpike, Antietam">
        </a>
        <a href="#" data-src="https://warofrights.com/Pages_FieldReports/Update166/Images/Carlton_battery_2-min.jpg">
            <img class="galleryImageContainer worTightBorder" src="https://warofrights.com/Pages_FieldReports/Update166/Images/Carlton_battery_2-min.jpg" alt="Campbell’s Battery (USA), Hagerstown Turnpike, Antietam">
        </a>
    </div>
    <div class="fancyClearBoth">
    </div>
</div>

@await Html.PartialAsync("_Gallery")

@section scripts {
    <script>
        $(document).ready(function () {
            $("#navMaps").addClass('active');
            $("#navMapAntietam").addClass('active');
        })
    </script>
}
