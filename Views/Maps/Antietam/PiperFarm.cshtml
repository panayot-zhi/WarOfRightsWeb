@{
    ViewBag.Title = Maps.PiperFarm;

    var map = Maps.PiperFarm;
    var dateTime = Maps.DateTimes[map];
    var image = Url.Content($"~/img/maps/{map}.png");
    var imageLoading = Url.Content($"~/img/maps/{map}_loading.png");

    var mapInfo = MapsInfo.Parameters[map];
}

@section navigation {
    @await Html.PartialAsync("_MapsNav")
}

<h2 class="contentTitle" id="tabTitle">Piper Farm</h2>

<div id="mapHistory" class="fancyClearBoth">

    <h2 class="text-right">History</h2>

</div>

<h2 class="text-right">@Maps.DateTimes[Maps.PiperFarm]</h2>
<a href="@Url.Content($"~/img/maps/{Maps.PiperFarm}.png")" target="_blank">
    <img src="@Url.Content($"~/img/maps/{Maps.PiperFarm}.png")" class="imageMaxWidth">
</a>

<p></p>

<a href="@Url.Content($"~/img/maps/{Maps.PiperFarm}_loading.png")" target="_blank">
    <img src="@Url.Content($"~/img/maps/{Maps.PiperFarm}_loading.png")" class="imageMaxWidth">
</a>


<h2 class="text-right">Belligerents</h2>

@section scripts {
    <script>
        $(document).ready(function () {
            $("#navMaps").addClass('active');
            $("#navMapAntietam").addClass('active');
        })
    </script>
}
